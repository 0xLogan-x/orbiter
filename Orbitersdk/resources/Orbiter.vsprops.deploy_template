		Value="$(DeployDir)\Config"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="ScenarioDir"
		Value="$(DeployDir)\Scenarios"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="SDKDir"
		Value="$(DeployDir)\Orbitersdk"
		PerformEnvironmentSet="true"
	/>
