		DataExecutionPrevention="0"
		ImportLibrary="$(OutDir)\$(ProjectName).lib"
		TargetMachine="1"
	/>
	<Tool
		Name="VCPreBuildEventTool"
		CommandLine="if
		&quot;$(OrbiterDir)&quot;==&quot;ORBITERDIR_UNDEFINED&quot; (&#x0D;&#x0A;echo .&#x0D;&#x0A;echo ==========================================&#x0D;&#x0A;echo !  OrbiterDir has not been defined.&#x0D;&#x0A;echo !  Please edit Orbitersdk\resources\orbiterroot.vsprops&#x0D;&#x0A;echo ==========================================&#x0D;&#x0A;echo .&#x0D;&#x0A;exit 1 )&#x0D;&#x0A;"
	/>
	<UserMacro
		Name="DeployDir"
		Value="$(OrbiterDir)"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="BuildDir"
		Value="$(DeployDir)\bin\Orbitersdk\$(ProjectName)"
