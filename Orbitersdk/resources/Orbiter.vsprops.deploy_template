	/>
	<UserMacro
		Name="ModuleDir"
		Value="$(DeployDir)\Modules"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="MeshDir"
		Value="$(DeployDir)\Meshes"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
