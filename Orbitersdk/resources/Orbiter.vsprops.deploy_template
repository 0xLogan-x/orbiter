		OutputFile=""
		LinkIncremental="1"
		AdditionalLibraryDirectories="$(SDKLibDir)"
		IgnoreDefaultLibraryNames=""
		SubSystem="2"
		OptimizeReferences="2"
		EnableCOMDATFolding="2"
