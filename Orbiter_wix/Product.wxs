    <Package Id="*" Keywords="Orbiter" Description="Orbiter 2016 Installer"
             Comments="Orbiter is a free Space Flight Simulator for Windows PCs"
             InstallerVersion="300" Compressed="yes" InstallScope="perUser" />
    <MediaTemplate EmbedCab="no" MaximumUncompressedMediaSize="2000"/>
    
    <Upgrade Id="63115016-042a-42d5-b1aa-7a4faa363e0e">
      <UpgradeVersion OnlyDetect="no" Property="PREVIOUSFOUND"
                      Minimum="1.0.0.0" IncludeMinimum="yes"
                      Maximum="1.1.0.0" IncludeMaximum="no" />
    </Upgrade>

    <InstallExecuteSequence>
      <RemoveExistingProducts After="InstallInitialize" />
    </InstallExecuteSequence>
    
    <!-- Installer GUI interface -->
    <!-- <Property Id="ALLUSERS" Value="1" /> -->
    <Property Id="WIXUI_INSTALLDIR" Value="APPLICATIONROOTDIRECTORY" />
    <UI>
      <UIRef Id="WixUI_Mondo" />
      <Publish Dialog="ExitDialog"
          Control="Finish"
          Event="DoAction"
          Value="LaunchApplication">WIXUI_EXITDIALOGOPTIONALCHECKBOX = 1 and NOT Installed
      </Publish>
    </UI>
    <Property Id="WIXUI_EXITDIALOGOPTIONALCHECKBOXTEXT" Value="Launch Orbiter 2016" />

    <!-- Include launch option -->
    <Property Id="WixShellExecTarget" Value="[#orbiter.exe]" />
    <CustomAction Id="LaunchApplication"
        BinaryKey="WixCA"
        DllEntry="WixShellExec"
        Impersonate="yes" />    
    
  <!-- GUI element variables -->
