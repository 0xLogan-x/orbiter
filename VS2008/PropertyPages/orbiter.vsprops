	/>
	<UserMacro
		Name="OrbiterSdkDir"
		Value="$(BuildDir)\Orbitersdk"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="OrbiterSdkIncludeDir"
		Value="$(OrbiterSdkDir)\include"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="OrbiterSdkLibDir"
		Value="$(OrbiterSdkDir)\lib"
